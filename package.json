    "babel-polyfill": "^6.20.0",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-root-import": "^4.1.5",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "testdouble": "^1.10.2"
  },
  "dependencies": {
    "moment": "^2.17.1",
    "prompt": "^1.0.0",
    "ramda": "^0.23.0",
    "request": "^2.79.0",
